generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int     @id @default(autoincrement())
  username String  @unique
  password String
  images   Image[]
}

model Image {
  id     Int    @id @default(autoincrement())
  url    String
  ownerId Int
  owner  User   @relation(fields: [ownerId], references: [id])
}